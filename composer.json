{
    "name": "arslanimamutdinov/iso-standard-3166",
    "authors": [
        {
            "name": "Arslan Imamutdinov",
            "email": "ara.imamutdinov@gmail.com"
        }
    ],
    "require": {
        "arslanimamutdinov/iso-standard-utilities": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12.90"
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/arslanim/iso-standard-utilities.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "arslanimamutdinov\\ISOStandard3166\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "arslanimamutdinov\\ISOStandard3166\\tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "sort-packages": true,
        "process-timeout": 1800,
        "github-oauth": {
            "github.com": "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"
        }
    }
}
